{
    "settings": {
        "http": {
            "header_read_timeout": 10,
            "body_read_timeout": 600,
            "send_timeout": 3600,
            "idle_timeout": 120,
            "max_body_size": 5368709120
        }
    },
    "listeners": {
        "*:8080": {
            "pass": "applications/api"
        }
    },
    "applications": {
        "api": {
            "type": "php",
            "user": "unit",
            "group": "unit",
            "root": "/app-src/public",
            "index": "index.php"
        }
    },
    "access_log": "/var/log/access.log"
}
